{
  "adobe_users": {
    "exclude_identity_types": [
      "adobeID"
    ],
    "exclude_adobe_groups": null,
    "exclude_users": null,
    "connectors": {
      "umapi": "connector-umapi.yml",
      "umapi_data": {
        "server": null,
        "enterprise": {
          "org_id": "Org ID goes here",
          "api_key": "API key goes here",
          "client_secret": "Client secret goes here",
          "tech_acct": "Tech account ID goes here",
          "priv_key_path": "path/to/private/key/file"
        }
      }
    }
  },
  "directory_users": {
    "user_identity_type": "enterpriseID",
    "default_country_code": "US",
    "connectors": {
      "ldap": "connector-ldap.yml",
      "ldap_data": {
        "username": "LDAP or Credential Manager username goes here",
        "password": "LDAP password goes here",
        "host": "LDAP host URL goes here.  e.g. ldap://ldap.example.com",
        "base_dn": "defines the base DN. e.g. DC=example,DC=com",
        "search_page_size": 200,
        "require_tls_cert": false,
        "all_users_filter": "(&(objectClass=user)(objectCategory=person)(!(userAccountControl:1.2.840.113556.1.4.803:=2)))",
        "group_filter_format": "(&(|(objectCategory=group)(objectClass=groupOfNames)(objectClass=posixGroup))(cn={group}))",
        "group_member_filter_format": "(memberOf={group_dn})",
        "user_email_format": "{mail}"
      }
    },
    "groups": [
      {
        "directory_group": "Finance",
        "adobe_groups": null
      },
      {
        "directory_group": "Administration",
        "adobe_groups": [
          "Acrobat DC Pro"
        ]
      },
      {
        "directory_group": "Newsletter",
        "adobe_groups": [
          "Copy Editors",
          "Acrobat DC Pro"
        ]
      }
    ]
  },
  "limits": {
    "max_adobe_only_users": 200
  },
  "logging": {
    "log_to_file": false,
    "file_log_directory": "logs",
    "file_log_level": "info",
    "console_log_level": "info"
  },
  "ustapp": {
    "basedir": ".\\tests\\fixture",
    "basefile": ".\\tests\\fixture\\user-sync-config.yml",
    "umapifile": ".\\tests\\fixture\\connector-umapi.yml",
    "ldapfile": ".\\tests\\fixture\\connector-ldap.yml"
  }
}
